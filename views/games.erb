  <div>
    <% if authorized? %>
      <h3>Partidas</h3>
      <div id="gamesModel" data-bind="with: gamesModel"> 
        <table class="table table-striped table-condensed" id="table-games">
          <thead>
            <tr>
                <th>Username</th>
                <th>Username</th>
                <th>Status</th>
                <th></th>                                        
            </tr>
          </thead>  
          <tbody data-bind="foreach: games">
            <tr>
              <td data-bind="text: username1"></td>
              <td data-bind="text: username2"></td>
              <td ><span class="label" data-bind="css: statusLabel"><span data-bind=" text:status"></span></span></td>
              <td><a class="glyphicon glyphicon-link"  data-toggle="tooltip" title="VER JUEGO" data-placement="left" data-bind="attr: { href: '/games/'+id }"></a></td>
            </tr>
          </tbody>
        </table>
      </div>
    <% end %>
    </div>